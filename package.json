{
  "name": "cetak-aja-online",
  "private": true,
  "version": "1.0.0",
  "description": "Aplikasi Point of Sale untuk Percetakan Dan Digital Printing",
  "keywords": [
    "sveltekit",
    "pos",
    "printing",
    "digital-printing",
    "point-of-sale",
    "order-management",
    "payment-processing",
    "prisma",
    "postgresql",
    "tailwind",
    "daisyui"
  ],
  "author": {
    "name": "Cetak Aja Online",
    "email": "admin@bits.co.id",
    "url": "https://github.com/bitscoid"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitscoid/cetakaja-go.git"
  },
  "bugs": {
    "url": "https://github.com/bitscoid/cetakaja-go/issues"
  },
  "homepage": "https://bits.co.id",
  "type": "module",
  "main": "build/index.js",
  "module": "build/index.js",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js"
    }
  },
  "files": [
    "build",
    "src",
    "prisma",
    "!**/*.test.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "tsx prisma/seed.ts",
    "reset": "rm -rf node_modules && rm -rf .svelte-kit && pnpm install",
    "bits": "pnpm install && pnpm prisma generate && pnpm prisma migrate dev --name init && pnpm seed",
    "bits:reset": "pnpm install && pnpm prisma generate && pnpm prisma migrate reset && pnpm seed",
    "bits:prod": "pnpm install && pnpm prisma generate && pnpm prisma migrate deploy && pnpm seed",
    "db:reset": "npx prisma migrate reset --force",
    "prepare": "svelte-kit sync || echo ''",
    "lint": "eslint . --ext .ts,.svelte --cache",
    "format": "prettier --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "echo \"No tests found\"",
    "test:unit": "echo \"No unit tests found\"",
    "test:integration": "echo \"No integration tests found\"",
    "test:e2e": "echo \"No end-to-end tests found\"",
    "test:coverage": "echo \"No coverage setup found\""
  },
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@sveltejs/adapter-auto": "^6.1.1",
    "@sveltejs/adapter-node": "^5.3.3",
    "@sveltejs/adapter-vercel": "^5.10.3",
    "@sveltejs/kit": "^2.46.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/chart.js": "^4.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.7.2",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "eslint": "^9.37.0",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prisma": "^6.17.1",
    "svelte": "^5.39.12",
    "svelte-check": "^4.3.3",
    "svelte-eslint-parser": "^1.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.46.1",
    "vite": "^7.1.9"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@tailwindcss/oxide",
      "core-js",
      "esbuild",
      "prisma",
      "sharp",
      "workerd"
    ]
  },
  "dependencies": {
    "@faker-js/faker": "^10.0.0",
    "@prisma/client": "^6.17.1",
    "@tailwindcss/vite": "^4.1.14",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.1",
    "cookie": "^1.0.2",
    "daisyui": "^5.3.0",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "tailwindcss": "^4.1.14",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  }
}
